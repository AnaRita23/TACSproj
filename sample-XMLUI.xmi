<?xml version="1.0" encoding="ISO-8859-1"?>
<amlUI:XMLPage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:amlUI="https://xmlUI/model" title="Main Page" height="800" width="600">
  <panels type="StackPanel">
    <buttons content="Submit" command="submitForm"/>
    <buttons content="Cancel" command="cancelForm"/>
    <comboBoxes selectedItem="Option1">
      <items text="Option1"/>
      <items text="Option2"/>
      <items text="Option3"/>
